{
  "name": "karin-plugin-template",
  "version": "1.0.0",
  "description": "karin plugin bilibili",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarinJS/karin-plugin-bilibili.git"
  },
  "author": "jack",
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "app": "karin start",
    "dev": "karin dev",
    "init": "karin init",
    "start": "karin start"
  },
  "devDependencies": {
    "neostandard": "^0.12.0"
  },
  "peerDependencies": {
    "node-karin": "1.0.3"
  },
  "karin": {
    "apps": [
      "lib/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "lib/**/*",
      "@karinjs/**/*"
    ]
  }
}
